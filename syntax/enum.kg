enum Sequence['T]
    Cons('T, Sequence['T]),
    Nil
end

record List['T]
    value: 'T,
    next: List['T]
end

function double['T](value: 'T): 'T
    value * 2
end

let test = double[int]
let nil_seq = Sequence[int].Nil
let cons_seq = Sequence[int].Cons(10, nil_seq)

let bool_list = Sequence[bool].Cons(true, Sequence[bool].Cons(false, Sequence[bool].Nil))

test(10)