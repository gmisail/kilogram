let double = function(x: int): int
	x * 2
end

let triple = function(x: int): int
	double(x) + x
end

let make_pair = function(x: int, y: int): { x: int, y: int }
	{ x: x, y: y } 
end

let double_pair = function(pair: { x: int, y: int }): int
	0
end

{ double: double, triple: triple, pair: double_pair(make_pair(1, 2)) }
