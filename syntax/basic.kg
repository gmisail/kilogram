let double = function(x: int): int
	x * 2
end

let triple = function(x: int): int
	double(x) + x
end

let make_point = function(x: int, y: int, z: int): { x: int, y: int, z: int }
	{ x: x, y: y, z: z } 
end

let make_2d_point = function(temp: { p: { x : int, y: int } }): { p: { x: int, y: int} }
	temp
end

make_2d_point({ p: { x: 0, y: 1, z: 2} })
