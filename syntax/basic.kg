extern int_to_string: (int) -> string
extern print: (string) -> int

let make_point = function(x: int, y: int, z: int): { x: int, y: int, z: int }
	{ x: x, y: y, z: z } 
end

let sum_point = function(point: { x: int, y: int, z: int }): int
	point.x + point.y + point.z
end

print(int_to_string(sum_point({ x: 1, y: 2, z: 3 })))
